@startuml
!include style.puml
skinparam ClassFontColor #000000
skinparam ClassBorderColor #000000
skinparam linetype ortho
hide empty methods
hide empty members

title Final state

class MarkCommand as "__:MarkCommand__"
together {
class DebtorIndex1 as "__1:Index__"
class DebtIndex2 as "__2:Index__"
class DebtIndex3 as "__3:Index__"
}

MarkCommand ----> "debtor index" DebtorIndex1
MarkCommand ----> "debt index" DebtIndex2
MarkCommand ----> "debt index" DebtIndex3
MarkCommand -right[hidden]-> DebtorIndex1
DebtIndex2 -right[hidden]-> DebtIndex3

@end
